{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Entities/*": ["src/entities/*"],
      "@Features/*": ["src/features/*"],
      "@Screens/*": ["src/screens/*"],
      "@Shared/*": ["src/shared/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
